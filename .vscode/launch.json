  "version": "0.2.0",
  "configurations": [
    {
      "name": "formatCsv",
      "program": "${workspaceFolder}/formatCsv.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Debug Sandbox",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\sandbox.js"
